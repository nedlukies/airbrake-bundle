{
    "name": "aminin/airbrake-bundle",
    "description": "Airbrake API v3 integration for Symfony 3/4/5",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Minin",
            "email": "anton.a.minin@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^3.4|^4.4|^5.0|^6.0",
        "airbrake/phpbrake": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/yaml": "^3.4|^4.4|^5.0",
        "squizlabs/php_codesniffer": "^3.0.2",
        "symfony/symfony": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Ami\\AirbrakeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ami\\AirbrakeBundle\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
